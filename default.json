{
  "description": "Default config to check every weekend",
  "extends": [
    "config:base",
    "group:allNonMajor",
    ":semanticCommitTypeAll(chore)"
  ],
  "timezone": "Europe/Paris",
  "schedule": [
    "after 2am and before 10am every saturday"
  ],
  "minimumReleaseAge": "1 day",
  "lockFileMaintenance": {
    "enabled": true,
    "schedule": [
      "after 2am and before 10am every saturday"
    ]
  },
  "reviewers": [
    "erwanjugand"
  ],
  "labels": [
    "dependencies"
  ],
  "commitBodyTable": true,
  "packageRules": [
    {
      "groupName": "Font Awesome",
      "matchPackagePrefixes": [
        "@fortawesome/"
      ],
      "matchUpdateTypes": [
        "major"
      ]
    }
  ],
  "hostRules": [
    {
      "matchHost": "https://npm.fontawesome.com/",
      "hostType": "npm",
      "encrypted": {
        "token": ""
      }
    }
  ]
}
